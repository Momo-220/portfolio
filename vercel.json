{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/js/$1"
        },
        {
            "src": "/assets/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
} 